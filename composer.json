{
    "name": "webcomposer/fe-mobile-entrypage",
    "license": "proprietary",
    "description": "Mobile Instance using Web Composer framework",
    "require": {
        "php": ">=7.1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "webcomposer/fe-core": "dev-release-v1.268.0",
        "nikolaposa/rate-limit": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\MobileEntry\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "kint-php/kint": "^1.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:sportion/marketing/websites/websites-commons/fe-core.git"
        }
    ],
    "scripts": {
        "int": [
            "@composer install",
            "@composer update webcomposer/fe-core"
        ],
        "upd": [
            "@composer update webcomposer/fe-core"
        ]
    },
    "config": {
        "secure-http": false,
        "cache-dir": ".composer-cache",
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "product": "webcomposer",
        "installer-types": ["library"],
        "installer-paths": {
            "core": ["webcomposer/fe-core"]
        }
    }
}
