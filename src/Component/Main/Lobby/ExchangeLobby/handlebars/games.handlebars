<ul class="game-exchange-lobby-container clearfix">
{{#each games}}
    <li class="game-tiles exchange-game-tile {{this.size}} {{this.overridesize}}">
        <a class="game-listing-item"
        {{#if ../isLogin}}
            data-game-launch="true"
        {{/if}}
        data-game-provider="{{this.game_provider}}"
        data-game-title="{{ this.title }}"
        data-game-target="{{ this.target }}"
        data-game-loader="{{ this.use_game_loader }}"
        data-game-lobby="true"
        {{#if this.tile_url}}
            href="{{ this.tile_url }}"
        {{/if}}
        data-game-maintenance="{{ this.game_maintenance}}"
        >
            {{#if this.ribbon}}
                <span class="game-ribbon lottery-ribbon"
                    style="background: {{ this.ribbon.background}}; color: {{ this.ribbon.color }};"
                >
                    {{ this.ribbon.name }}
                </span>
            {{/if}}
            <picture>
                <source srcset="{{ this.img_landscape }}" media="(min-width: 40em) and (orientation: landscape)">
                <img
                    src="{{ this.image.url }}"
                    alt="{{ this.image.alt }}"
                    class="w-100"
                >
            </picture>
        </a>
        {{#if this.game_maintenance}}
            <div class="exchange-tile-maintenance">
                <p>{{ this.game_maintenance_text }}</p>
            </div>
        {{/if}}
    </li>
{{/each}}
</ul>
<div id="game-loader" class="game-loader">
    <span class="game-loader-load-more">Load More</span>
    <div class="sk-fading-circle mobile-game-loader hidden">
        <div class="sk-fading-circle">
            <div class="sk-circle1 sk-circle"></div>
            <div class="sk-circle2 sk-circle"></div>
            <div class="sk-circle3 sk-circle"></div>
            <div class="sk-circle4 sk-circle"></div>
            <div class="sk-circle5 sk-circle"></div>
            <div class="sk-circle6 sk-circle"></div>
            <div class="sk-circle7 sk-circle"></div>
            <div class="sk-circle8 sk-circle"></div>
            <div class="sk-circle9 sk-circle"></div>
            <div class="sk-circle10 sk-circle"></div>
            <div class="sk-circle11 sk-circle"></div>
            <div class="sk-circle12 sk-circle"></div>
        </div>
    </div>
</div>