{% if otherlinks %}
<ul class="mobile-menu-footer clearfix">
    {% for menu in otherlinks %}
            <li class="mobile-menu-item">
                <a
                    href="{{ url(menu.alias) }}"
                    class="mobile-menu-link menu-item-internal {{ menu.attributes.class }}"
                    target="{{ menu.attributes.target }}"
                    data-router="true"
                    data-router-refresh='["main", "language"]'
                    data-router-active-link-class="active"
                >
                <span>{{ menu.title }}</span>
                </a>
            </li>
        {% endfor %}
</ul>
{% endif %}
